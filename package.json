{
  "name": "yarn-berry-test",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build",
    "serve:dev": "yarn build:dev && vite preview",
    "serve:prod": "yarn build:prod && vite preview",
    "gen": "graphql-codegen --config codegen.yml && sed -i '' 's/= unknown/extends unknown/g' src/generated/graphql.tsx && sed -i '' 's/TData =/TData extends/g' src/generated/graphql.tsx"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.8",
    "daisyui": "^2.24.0",
    "dayjs": "^1.11.5",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6",
    "i18next": "^21.9.1",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-i18next": "^11.18.5",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.8",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-query": "^4.0.1",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
  }
}
